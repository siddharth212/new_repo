pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World.......'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        
        stage('Deploying') {
            steps {
                echo 'Deploying the codegg'
            }
        }
        
        stage('Testing') {
            steps {
                echo 'Testing the code ...'
            }
        }
        
        stage('Release') {
            steps {
                echo 'Releasing the Code..'
            }
        }
    }
}
